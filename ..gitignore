.destination >h1{
    padding: 50px;
    text-align: center;
    font-family: Arial, Helvetica, sans-serif;
    font-size: 4rem;
    color: rgb(77, 77, 193);
}

.gallery{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    flex-wrap: wrap;
    gap: 5px;
    padding: 10px;
    width: 90%;
    margin-left: 90px;
}

.gallery-conteneurs {
    width: 350px;
    margin-bottom: 20px;
    box-shadow: 0 1px 3px rgba(34, 36, 34, 0.744), 0 1px 2px rgba(0, 0, 0, 0.822);
    transition: transform 0.7s; 
    border-radius: 15px;
}

.gallery-conteneurs:hover{
    border: 1px solid rgb(240, 234, 234);
    transform: translateY(-15px);
}

.gallery-image h2{
    font-family: Arial, Helvetica, sans-serif;
}

.gallery-image span{
    background: gray;
    font-size: 3rem;
    
}


.gallery-image {
    text-align: center; 
    flex-direction: row;
}

.gallery-image img {
    width: 100%;
    border-radius: 15px;
}


